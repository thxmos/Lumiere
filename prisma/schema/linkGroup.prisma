model LinkGroup {
  id          String  @id @unique @default(uuid())
  name        String
  description String?

  links Link[] @relation("linkGroupLinks")

  createdAt DateTime  @default(now())
  updatedAt DateTime  @default(now()) @updatedAt
  deletedAt DateTime?

  userId String
  user   User   @relation("UserLinkGroups", fields: [userId], references: [id])
}
